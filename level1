import spacy
nlp = spacy.load("en_core_web_sm")
sent = "The players were determined"
subj = aux_verb = root_verb = obj = None
list_sent=[]
doc = nlp(sent)
for token in doc:
  if obj!=None:
    list_sent.append(token)
    continue
  print(token.dep_)
  if (token.dep_ == "nsubjpass") :
    subj = token
  if (token.dep_ == "ROOT"):
    root_verb = token
  if (token.dep_ == "auxpass"):
    aux_verb = token
  if (token.dep_ == "pobj"):
    obj = token
  
print("what",aux_verb,subj,root_verb)
for item in list_sent:
  print(item)
